package com.keduit;

public class CharaterTypeUniCode4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("AB" + '\b' + 'C');
		System.out.println("AB" + '\t' + 'C');
		System.out.println("AB" + '\n' + 'C');
		System.out.println("AB" + '\r' + 'C');
	}

}
