package com.keduit;

public class CharaterTypeUniCode3 {
	
	public static void main(String[] args) {
	
		int a,b;
		char c,d;
		
		a = 0xF90A; //금
		b = 0x50; //p
		
		
		//정수형 변수 a를 문자형으로 출력한다.
		System.out.printf("%c \n", (char)a);
		
		c = (char)b;
		// 문자형 변수 c를 문자형으로 출력된다.
		
		System.out.printf("%c \n",(char)c);
	
		
		//문자형 변수 d를 두가지 형태로 출력한다.
		d= '#';
		System.out.printf("%c의 ASCii값은 %d입니다.", d, (int)d);
		System.out.printf("%d \n", (int)d );
		System.out.printf("%c \n", d );
	
	}

}
