		if(jeon >= 70) {
			if((kyo >= 30 && normal>= 30) || (kyo + normal) >= 80) {
				String degreeOk = (jeon + kyo + normal) >= 140 ? "졸업가능" : "졸업 불가능";
				System.out.println(degreeOk);
			}else {
				System.out.println("졸업 불가능");
			}
		}else {
			System.out.println("졸업 불가능");
		}
		
		
		
		
	}
	
